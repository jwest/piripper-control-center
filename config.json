{
    "cdromStatus": {
        "setcdCommand": "setcd",
        "ejectCommand": "eject"
    },
    "whipperWrapper": {
        "whipperCommand": "/usr/bin/docker run --rm --device=/dev/cdrom -v /home/pi/.config/whipper:/home/worker/.config/whipper -v {{WORKSPACE_PATH}}/output:/output whipperteam/whipper"
    },
    "fileNameNormalizator": {
        "enabled": true
    },
    "albumStore": {
        "destinations": [
            { "path": "~/Music" }
        ]
    }
}